{
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
  },
  "rules": {
      "semi": "error"
  },
  "plugins": [
    "security",
    "sonarjs"
  ],
  "extends": [
    "plugin:security/recommended",
    "plugin:sonarjs/recommended"
  ]
}
